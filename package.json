{
  "name": "LMS",
  "version": "1.0.0",
  "description": "My posh Nuxt.js project",
  "author": "Kouta Gakuhari",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "build:stg": "cross-env NODE_ENV=\"staging\" nuxt-ts build",
    "build:prod": "cross-env NODE_ENV=\"production\" nuxt-ts build",
    "start": "nuxt-ts start",
    "start:stg": "cross-env NODE_ENV=\"staging\" nuxt-ts start",
    "start:prod": "cross-env NODE_ENV=\"production\" nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.2.2",
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/onesignal": "^3.0.0-beta.16",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^6.0.3",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^2.5.0",
    "vue2-touch-events": "^2.0.0",
    "vuex-persistedstate": "^2.7.0",
    "yarn": "^1.19.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.2",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "vue-property-decorator": "^8.3.0"
  }
